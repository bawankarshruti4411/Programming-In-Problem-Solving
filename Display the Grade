m1 = float(input("subject 1: "))
m2 = float(input("subject 2: "))
m3 = float(input("subject 3: "))
m4 = float(input("subject 4: "))
m5 = float(input("subject 5: "))
AM = (m1+m2+m3+m4+m5)/5.0
print(f"Average Marks: {AM:.2f}")
if(AM>=90 and AM<=100):
	print("Grade: A\n")
elif(AM>=80 and AM<=89):
	print("Grade: B\n")
elif(AM>=70 and AM<=79):
	print("Grade: C")
elif(AM>=60 and AM<=69):
	print("Grade: D")
else:
	print("Grade: F\n")
